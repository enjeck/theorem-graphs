{
  "homepage": "http://enjeck.github.io/theorem-graphs",
  "name": "theorem-graphs",
  "version": "1.0.0",
  "description": "Graphs depicting relationships between various math theorems",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html --public-url ./",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "jquery": "^3.6.0",
    "ngraph.graph": "^19.1.0",
    "parcel-bundler": "^1.6.1",
    "vivagraphjs": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.0",
    "gh-pages": "^3.2.3"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "weight",
    "mathematical modeling",
    "differential equations",
    "ODEs"
  ]
}
